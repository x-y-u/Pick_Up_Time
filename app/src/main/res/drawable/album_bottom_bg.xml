<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <solid android:color="#FFE4C4" />
        </shape>
    </item>

    <item
        android:end="-40dp"
        android:gravity="bottom"
        android:start="-40dp"
        android:bottom="-60dp">
        <shape
            android:shape="oval"
            android:useLevel="true">
            <size android:height="140dp" />
            <stroke
                android:color="#12C4B2"
                android:width="9dp"/>
        </shape>
    </item>

</layer-list>